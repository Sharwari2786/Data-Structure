#include <stdio.h>
#include <stdlib.h>

typedef struct node {
    int data;
    struct node *left, *right;
} *BST;

BST getNode(int val) {
    BST newNode = (BST)malloc(sizeof(struct node));
    newNode->data = val;
    newNode->left = newNode->right = NULL;
    return newNode;
}

BST insert(BST root, int val) {
    if (root == NULL)
        return getNode(val);
    
    if (val < root->data)
        root->left = insert(root->left, val);
    else if (val > root->data)
        root->right = insert(root->right, val);
    
    return root;


void inorder(BST root) {
    if (root != NULL) {
        inorder(root->left);
        printf("%d ", root->data);
        inorder(root->right);
    }
}

void printRange(BST root, int low, int high) {
    if (root == NULL)
        return;

    if (root->data > low)
        printRange(root->left, low, high);

    if (root->data >= low && root->data <= high)
        printf("%d ", root->data);

    if (root->data < high)
        printRange(root->right, low, high);
}

int main() {
    BST root = NULL;
    int n, val, low, high;

    printf("Enter number of nodes to insert in BST: ");
    scanf("%d", &n);

    printf("Enter %d values: ", n);
    for (int i = 0; i < n; i++) {
        scanf("%d", &val);
        root = insert(root, val);
    }

    printf("\nInorder Traversal (sorted): ");
    inorder(root);

    printf("\n\nEnter the range (low high): ");
    scanf("%d %d", &low, &high);

    printf("Nodes in range [%d, %d]: ", low, high);
    printRange(root, low, high);

    return 0;
}
